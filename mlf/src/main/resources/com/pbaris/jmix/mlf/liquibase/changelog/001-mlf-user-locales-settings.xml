<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

	<changeSet author="mlf" id="1" context="!cuba">
		<preConditions onFail="CONTINUE">
			<customPrecondition className="com.pbaris.jmix.mlf.liquibase.AppSettingsPrecondition" />
		</preConditions>

		<createTable tableName="MLF_USER_LOCALES_SETTINGS">
			<column name="ID" type="INT">
				<constraints nullable="false" primaryKey="true" primaryKeyName="PK_MLF_USER_LOCALES_SETTINGS"/>
			</column>
			<column name="VERSION" type="INT">
				<constraints nullable="false"/>
			</column>
			<column name="LOCALES" type="VARCHAR(255)"/>
			<column name="CREATED_BY" type="VARCHAR(50)"/>
			<column name="CREATE_TS" type="datetime"/>
			<column name="UPDATED_BY" type="VARCHAR(50)"/>
			<column name="UPDATE_TS" type="datetime"/>
		</createTable>
	</changeSet>
</databaseChangeLog>
